
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<input type="text" name="name1" id="txt1" value="" />
<input type="button" name="name2" id="btn1" value="Get Item by Id" />
<input type="button" name="name3" id="btn2" value="Get All Items" />
<ul id="names"></ul>
<script>
    $(document).ready(function () {

        $("#btn2").click(function () {

            $.getJSON("/api/Web/", function (data) {

                $.each(data, function (key, val) {
                    $("<li>" + val + "</li>").appendTo($("#names"));

                })

            })

        })

    })         
</script>